{
  "name": "pcdashboardmetrics",
  "version": "0.1.0",
  "private": true,
  "description": "PC Dashboard Metrics",
  "main": "public/main.js",
  "build": {
    "appId": "com.ricardopimentel.pc-dashboard-metrics",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "build/icon.*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "./build/icon.png"
    }
  },
  "author": "Ricardo Pimentel",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "bulma": "^0.8.0",
    "child-process": "^1.0.2",
    "concurrently": "^5.1.0",
    "electron-is-dev": "^1.1.0",
    "fast-xml-parser": "^3.15.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "react-sidebar": "^3.0.2",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "repeat": "^1.2.0",
    "wait-on": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "pack": "electron-builder --dir",
    "dist-windows": "electron-builder -w",
    "dist-linux": "electron-builder -l",
    "dist-arm64": "electron-builder --arm64"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1"
  }
}